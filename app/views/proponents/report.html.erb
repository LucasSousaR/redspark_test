<h1>Relatório de Funcionários</h1>

<table>
  <thead>
  <tr>
    <th>Faixa Salarial</th>
    <th>Quantidade</th>
  </tr>
  </thead>
  <tbody>
  <% @tracks.each do |faixa, quantidade| %>
    <tr>
      <td><%= faixa %></td>
      <td><%= quantidade %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= line_chart @tracks.map { |faixa, quantidade| { name: faixa, data: quantidade } } %>